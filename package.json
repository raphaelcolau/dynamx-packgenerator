{
  "name": "dynamx-package-manager",
  "version": "2.4.2",
  "description": "Tools for parsing, updating and selling dynamx package",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "node build.js",
    "build:win": "node build.js && node ./win-build/win-build.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "DynamX-PackManager": "src/index.ts"
  },
  "author": "Raphael Colau",
  "license": "ISC",
  "packageManager": "pnpm@10.26.2",
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "archiver": "^5.3.1",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "form-data": "^4.0.0",
    "glob": "^10.3.1"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/node": "^25.2.3",
    "esbuild": "^0.25.0",
    "postject": "^1.0.0-alpha.6",
    "rcedit": "^5.0.2",
    "resedit": "^3.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
